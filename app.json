{
  "expo": {
    "name": "land.original.superhumans",
    "slug": "OL-SUPERHUMANS",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/superhumans.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/superhumans.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "land.original.superhumans"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/superhumans.png",
        "backgroundColor": "#ffffff"
      },
      "package": "land.original.superhumans"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/superhumans.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-av",
        {
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone."
        }
      ]      
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "cf217d63-51cb-4c18-b4a0-9a2b7b94856e"
      }
    }
  }
}
